{
  "name": "angular-starter-es6-webpack",
  "version": "1.0.3",
  "description": "An Application Shell for generating Angular Applications with Webpack and ES6",
  "main": "app.js",
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack-dev-server --devtool=source-map --progress --colors --inline --hot --port=9000 --content-base app",
    "test": "set NODE_ENV=test&& karma start",
    "build": "set NODE_ENV=production&& npm start && cp ./app/index.html dist/index.html",
    "start": "webpack"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6"
  ],
  "author": "Sean Larkin",
  "license": "ISC",
